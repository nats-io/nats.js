{
  "name": "@nats-io/nats-core",
  "version": "3.0.0-54",
  "exports": {
    ".": "./src/mod.ts",
    "./internal": "./src/internal_mod.ts"
  },
  "publish": {
    "exclude": [
      "./.npmrc",
      "./build/",
      "./examples/",
      "./import_map.json",
      "./node_modules/",
      "./package-lock.json",
      "./package.json",
      "./tests/",
      "./tsconfig.json",
      "./typedoc.json",
      "./unsafe_tests/"
    ]
  },
  "lint": {
    "exclude": [
      "lib/"
    ]
  },
  "fmt": {
    "exclude": [
      "lib/"
    ]
  },
  "tasks": {
    "clean": "rm -rf ./lib ./cjs ./esm",
    "test": "deno test -A --parallel --reload tests/ --import-map=./import_map.json"
  },
  "imports": {
    "@nats-io/nkeys": "jsr:@nats-io/nkeys@~2.0.2",
    "@nats-io/nuid": "jsr:@nats-io/nuid@~2.0.3"
  }
}
