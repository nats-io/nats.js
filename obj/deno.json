{
  "name": "@nats-io/obj",
  "version": "3.0.0-1",
  "exports": {
    ".": "./src/mod.ts",
    "./internal": "./src/internal_mod.ts"
  },
  "publish": {
    "include": [
      "./src"
    ],
    "exclude": [
      "./tests"
    ]
  },
  "lint": {
    "exclude": [
      "lib/"
    ]
  },
  "fmt": {
    "exclude": [
      "lib/"
    ]
  },
  "tasks": {
    "test": "deno test -A --parallel --reload --quiet tests/ --import-map=import_map.json"
  },
  "imports": {
    "@nats-io/nats-core": "jsr:@nats-io/nats-core@~3.0.0-18",
    "@nats-io/jetstream": "jsr:@nats-io/jetstream@~3.0.0-4"
  }
}
